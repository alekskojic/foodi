<html>
    <head>
        <title>Foodi: Home</title>
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.7/css/materialize.min.css">
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
        <link rel="stylesheet" href="/styles.css">
        <script src="https://code.jquery.com/jquery-3.1.1.js"   integrity="sha256-16cdPddA6VdVInumRGo6IbivbERE8p7CQR3HzTBuELA="   crossorigin="anonymous"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.7/js/materialize.min.js"></script>
        <script type="text/javascript" src="script.js"></script>
    </head>


    <body>
        <div class="text">
        <div class="container">
            <img class="logo" src="foodi_kid.png">
            <h1>Foodi</h1>
            <h5>Cheap | Local | Awesome <i class="small material-icons">thumb_up</i></h5>
            <div class="form-wrap">
                <form class="search-form">
                    <div class="input-field">
                        <input placeholder="Enter Location" id="search" type="text">
                        <div class="wrapper">
                            <button class="search btn waves-effect waves-light" type="submit">Search</button>
                            <button id="current-location-button" class="waves-effect waves-light btn">
                                <i class="material-icons right">my_location</i> Use Current Location
                            </button>
                        </div>
                        <br>
                        <div id="preloader">
                        </div>
                    </div>
                </form>
            </div>
            </div>
        </div>
    </body>
</html>

